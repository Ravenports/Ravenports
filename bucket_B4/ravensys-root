			libusb20_desc.h \
			libutil.h \
			link.h \
			linker_set.h \
			login_cap.h \
			luks.h \
			memory.h \
			mntopts.h \
			mpool.h \
			netconfig.h \
			nlist.h \
			osreldate.h \
			paths.h \
			printf.h \
			pthread_np.h \
			re_comp.h \
			readpassphrase.h \
			res_update.h \
			resolv.h \
			runetype.h \
			sdp.h \
			stdalign.h \
			stdnoreturn.h \
			stringlist.h \
			sysexits.h \
			syslog.h \
			timeconv.h \
			timers.h \
			ttyent.h \
			uchar.h \
			ucontext.h \
			usb.h \
			usbhid.h \
			util.h \
			uuid.h \
			vis.h \
			xlocale.h \
			# end

headers_FreeBSD=	${headers_DragonFly:Nutil.h:Ndevattr.h:Ndfregress.h:Ndisktab.h:Nevtr.h:Nlibcryptsetup.h:Nluks.h:Nmntopts.h:Nre_comp.h:Nutmp.h} \
			_ctype.h \
			bsm/audit.h \
			libusb20.h \
			libusb20_desc.h \
			kenv.h \
			libgeom.h \
			libprocstat.h \
			malloc_np.h \
			jail.h \
			# end

headers_Linux_dirs=	acl \
			arpa \
			attr \
			asm-generic \
			drm \
			linux \
			net \
			netash \
			netatalk \
			netax25 \
			neteconet \
			netinet \
			netipx \
			netiucv \
