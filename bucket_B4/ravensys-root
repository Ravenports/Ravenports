	/usr/lib/libc.a \
	/usr/lib/libm.a \
	/usr/lib/librt.a \
	/usr/lib/libutil.a \
	/usr/lib/libcrypt.a \
	/usr/lib/libcompat.a \
	/usr/lib/libdevmapper.a \
	/usr/lib/libresolv.a \
	/usr/lib/librpcsvc.a \
	/usr/lib/libpthread.a \
	/usr/lib/libkvm.a \
	/usr/lib/libusbhid.a \
	/usr/lib/libx86_64.a \
	/usr/lib/libpci.a \
	/usr/lib/libossaudio.a \
	/usr/lib/libposix.a \
	/usr/lib/libperfuse.a \
	/usr/lib/libprop.a \
	/usr/lib/libpuffs.a \
	/usr/lib/libquota.a \
	/usr/lib/crt0.o \
	/usr/lib/crtbegin.o \
	/usr/lib/crtbeginS.o \
	/usr/lib/crtbeginT.o \
	/usr/lib/crtend.o \
	/usr/lib/crtendS.o \
