# For FreeBSD

Libexec_FreeBSD=	/libexec/ld-elf.so.1

Bin_FreeBSD=\
	/bin/[ \
	/bin/chflags \
	/bin/chmod \
	/bin/cp \
	/bin/date \
	/bin/echo \
	/bin/expr \
	/bin/hostname \
	/bin/kill \
	/bin/link \
	/bin/ln \
	/rescue/ls \
	/bin/mkdir \
	/bin/mv \
	/bin/pax \
	/bin/pwd \
	/bin/realpath \
	/bin/rm \
	/bin/rmdir \
	/rescue/sh \
	/bin/sleep \
	/bin/test \
	/bin/unlink \
	# end

