			${MKDIR} "$$DEST" ; \
		else \
			${INSTALL_DATA} "$$src" "$$DEST" ; \
		fi \
	done)
	cd ${WRKSRC} && ${COPYTREE_SHARE} docs ${STAGEDIR}${PREFIX}/share/netbox/docs
	${INSTALL_DATA} ${WRKSRC}/mkdocs.yml ${STAGEDIR}${PREFIX}/share/netbox
