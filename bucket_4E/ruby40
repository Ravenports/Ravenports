DISTFILE[1]=		ruby-4.0.1.tar.xz:main
DIST_SUBDIR=		ruby
DF_INDEX=		1
SPKGS[std]=		set
			primary
			docs
			examples
			man
			rinfo

OPTIONS_AVAILABLE=	none
OPTIONS_STANDARD=	none

