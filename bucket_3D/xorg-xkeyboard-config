HOMEPAGE=		https://www.x.org/
CONTACT=		nobody

DOWNLOAD_GROUPS=	main
SITES[main]=		XORG/individual/data/xkeyboard-config
DISTFILE[1]=		xkeyboard-config-2.21.tar.bz2:main
DIST_SUBDIR=		xorg
DF_INDEX=		1
SPKGS[standard]=	single

OPTIONS_AVAILABLE=	none
OPTIONS_STANDARD=	none

BUILD_DEPENDS=		libxslt:single:standard

USES=			gmake perl:build intltoolfix gettext-tools pkgconfig
GNOME_COMPONENTS=	intltool
XORG_COMPONENTS=	x11

DISTNAME=		xkeyboard-config-2.21

FPC_EQUIVALENT=		x1l/xkeyboard-config

MUST_CONFIGURE=		gnu
CONFIGURE_ARGS=		--with-xkb-base={{PREFIX}}/share/X11/xkb
			--with-xkb-rules-symlink=xorg

MAKE_ARGS=		pkgconfigdir={{PREFIX}}/lib/pkgconfig

INSTALL_TARGET=		install-strip

post-patch:
	@${REINPLACE_CMD} -e 's|/bin/bash|/bin/sh|g' -e 's|==|=|g' \
		${WRKSRC}/rules/merge.sh

post-install:
	@${MKDIR} ${STAGEDIR}/var/lib/xkb
	@${LN} -sf /var/lib/xkb ${STAGEDIR}${PREFIX}/share/X11/xkb/compiled

[FILE:299:descriptions/desc.single]
The non-arch keyboard configuration database for X Window.
The goal is to provide the consistent, well-structured,
frequently released open source of X keyboard configuration
data for X Window System implementations (free, open source
and commercial). The project is targetted to XKB-based systems.


