[FILE:7165:files/netbsd.mk]
# For NetBSD

Libexec_NetBSD=		/libexec/ld.elf_so

# no realpath in NetBSD

Bin_NetBSD=\
	/bin/[ \
	/usr/bin/chflags \
	/bin/chmod \
	/bin/cp \
	/bin/echo \
	/bin/expr \
	/bin/hostname \
	/bin/kill \
	/usr/sbin/link \
	/bin/ln \
	/bin/ls \
	/bin/mkdir \
	/bin/mv \
	/bin/pax \
	/bin/pwd \
	/bin/rm \
	/bin/rmdir \
	/bin/sh \
	/bin/sleep \
	/bin/test \
	/usr/sbin/unlink \
	# end

# chgrp is /bin/symlink to /sbin/chown
# no tsort

