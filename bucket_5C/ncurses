		${WRKSRC}/misc/Makefile.in
	${REINPLACE_CMD} -e 's,/usr/include,${PREFIX}/include,g' ${WRKSRC}/misc/ncurses-config.in

post-install:
	${STRIP_CMD} ${STAGEDIR}${PREFIX}/lib/*.so
	${MKDIR} ${STAGEDIR}${STD_DOCDIR}
	@for f in ${FILES_DOCS}; do \
	  ${INSTALL_DATA} ${WRKSRC}/$$f ${STAGEDIR}${STD_DOCDIR}/; \
	  ${ECHO} installed document $$f; \
	done
	# create the terminfo database
	${CP} ${WRKSRC}/lib/libncurses.so* ${LOCALBASE}/lib
	${CP} ${WRKSRC}/lib/libtinfo.so* ${LOCALBASE}/lib
	${MKDIR} ${STAGEDIR}${PREFIX}/share/terminfo
	${MKDIR} ${STAGEDIR}${PREFIX}/share/site-terminfo
	${WRKSRC}/progs/tic -sx -o ${STAGEDIR}${PREFIX}/share/terminfo ${WRKDIR}/terminfo.src

[FILE:382:descriptions/desc.primary]
The ncurses software includes a SVr4 and XSI-Curses compatible
curses library as well as terminfo tools including "tic", "infocmp",
and "captoinfo".  The library is used by other programs for text-mode
support of color, multiple highlights, forms-drawing characters,
automatic recognition of keypad and function-key sequences, and more.

This package contains the shared libraries.


[FILE:53:descriptions/desc.tools]
This package contains the terminfo tools of ncurses.


[FILE:56:descriptions/desc.terminfo]
This package contains the terminfo tree-based database.


