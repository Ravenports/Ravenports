assemble_pic:
	(cd ${WRKSRC} &&\
	 ${AR} -cq libtinfo_pic.a ${TINFO_OBJS} &&\
	 ranlib libtinfo_pic.a)
	(cd ${WRKSRC} &&\
	 ${AR} -cq libncurses_pic.a ${NCURSES_OBJS} &&\
	 ranlib libncurses_pic.a)

install_pic:
	${INSTALL_DATA} ${WRKSRC}/libtinfo_pic.a ${WRKSRC}/libncurses_pic.a \
		${STAGEDIR}${PREFIX}/lib/

