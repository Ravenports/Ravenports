pre-install:
	${FIND} ${WRKSRC} -type f \( -name \*.orig -o -name \*.bak \) -delete

post-install:
	${STRIP_CMD} ${STAGEDIR}${PREFIX}/bin/ncat
	${STRIP_CMD} ${STAGEDIR}${PREFIX}/bin/nmap
	${STRIP_CMD} ${STAGEDIR}${PREFIX}/bin/nping

post-configure:
