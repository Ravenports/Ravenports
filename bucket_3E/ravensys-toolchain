# Buildsheet autogenerated by ravenadm tool -- Do not edit.

NAMEBASE=		ravensys-toolchain
VERSION=		1.0
KEYWORDS=		raven
VARIANTS=		standard
SDESC[standard]=	Compiler and binutils toolchain for Ravenports
HOMEPAGE=		none
CONTACT=		John_Marino[draco@marino.st]

DOWNLOAD_GROUPS=	none
SPKGS[standard]=	single

OPTIONS_AVAILABLE=	none
OPTIONS_STANDARD=	none

BUILD_DEPENDS=		binutils:single:ravensys

PREFIX=			{{LOCALBASE}}/share/raven/toolchain

INVALID_RPATH=		yes

SKIP_BUILD=		yes
MAKE_ENV=		COMPVER=6.20170202
			TARGET={{CONFIGURE_TARGET}}

PLIST_SUB=		COMPVER=6.20170202
			GCCVER=6.3.1
			TARGET={{CONFIGURE_TARGET}}

do-extract:
	${MKDIR} ${WRKSRC}
	${CP} ${FILESDIR}/Makefile ${WRKSRC}/

post-install:
	@${FIND} ${STAGEDIR}${PREFIX}/lib ! -type d | \
		${SED} -e 's,^${STAGEDIR}${PREFIX}/,,' \
		>> ${WRKDIR}/.manifest.single.mktmp

[FILE:179:descriptions/desc.single]
This assembles a toolchain of the default compiler and binutils, and
relocates it to the share/raven directory.  It's purpose is to be the
toolchain mount for build environments.


