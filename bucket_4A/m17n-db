DISTFILE[2]=		glibc-2.26.tar.xz:gnu
DF_INDEX=		1 2
SPKGS[standard]=	single

OPTIONS_AVAILABLE=	none
OPTIONS_STANDARD=	none

USES=			gmake pkgconfig

FPC_EQUIVALENT=		devel/m17n-db

MUST_CONFIGURE=		gnu
