	${MKDIR} ${STAGEDIR}${PREFIX}/share/xml/docbook/5.1
	(cd ${WRKDIR}/docbook-xml_7/schemas && \
	${COPYTREE_SHARE} . ${STAGEDIR}${PREFIX}/share/xml/docbook/5.1)
	xmlcatmgr -c ${STAGEDIR}${PREFIX}/share/xml/docbook/catalog.xml \
	    add nextCatalog "${PREFIX}/share/xml/docbook/5.1/catalog.xml"; \

