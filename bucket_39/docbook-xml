pre-install:
	@${MKDIR} ${STAGEDIR}${PREFIX}/share/xml/docbook
	${ECHO_CMD} "CATALOG \"${LOCALBASE}/share/xml/xmlcharent/catalog\"" \
	    > ${STAGEDIR}${PREFIX}/share/xml/docbook/catalog
	xmlcatmgr -c ${STAGEDIR}${PREFIX}/share/xml/docbook/catalog.xml create
	xmlcatmgr -c ${STAGEDIR}${PREFIX}/share/xml/docbook/catalog.xml \
	    add nextCatalog "${LOCALBASE}/share/xml/xmlcharent/catalog.xml"

post-install:
	${INSTALL_DATA} ${FILESDIR}/catalog412.xml \
	    ${STAGEDIR}${PREFIX}/share/xml/docbook/4.1.2/catalog.xml
	# fix docbook URL
	${GREP} -rl '/docbook.sourceforge.net/' ${STAGEDIR} | ${XARGS} \
		${SED} -i'' -e 's|/docbook.sourceforge.net/|/cdn.docbook.org/|g'

post-extract:
	${MV} ${WRKDIR}/docbook-5.0.1 ${WRKDIR}/docbook-xml_6

do-install:
.for DF in 1 2 3 4 5 6
	D=${DL_SITES_${DISTFILE_${DF}:C/.*://}:H:T}; \
	${MKDIR} ${STAGEDIR}${PREFIX}/share/xml/docbook/$$D; \
	(cd ${WRKDIR}/docbook-xml_${DF} && \
	${COPYTREE_SHARE} . ${STAGEDIR}${PREFIX}/share/xml/docbook/$$D); \
	xmlcatmgr -c ${STAGEDIR}${PREFIX}/share/xml/docbook/catalog.xml \
	    add nextCatalog "${PREFIX}/share/xml/docbook/$$D/catalog.xml"; \
	if [ -f "${STAGEDIR}${PREFIX}/share/xml/docbook/$$D/docbook.cat" ]; then \
	    ${ECHO_CMD} "CATALOG \"${PREFIX}/share/xml/docbook/$$D/docbook.cat\"" \
	    >> ${STAGEDIR}${PREFIX}/share/xml/docbook/catalog; \
	fi
.endfor
